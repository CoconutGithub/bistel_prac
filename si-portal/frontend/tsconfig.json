{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "ES6",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "global.d.ts"]
}
